@model IEnumerable<AdmAspNet.Models.ViewModels.NeighbourViewModel>

@{
    ViewBag.Title = "Naboer";
}
@section sidebar {
    @Html.Partial("_SidebarPartial")
}
<h2>Naboer</h2>

<p>
    @Html.ActionLink("Opprett ny", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Distance)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PathPoint1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PathPoint2)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Distance)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PathPoint1.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PathPoint2.Id)
        </td>
        <td>
            @Html.ActionLink("Rediger","Edit",new { id = item.Id }) |
            @Html.ActionLink("Slett", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
