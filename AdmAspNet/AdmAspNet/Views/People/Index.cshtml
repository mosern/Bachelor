@model IEnumerable<AdmAspNet.Models.ViewModels.PeopleViewModel>

@{
    ViewBag.Title = "Personer";
}
@section sidebar {
    @Html.Partial("_SidebarPartial")
}
<h2>Personer</h2>

<p>
    @Html.ActionLink("Opprett ny", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JobTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TlfOffice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TlfMobile)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JobTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TlfOffice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TlfMobile)
        </td>
        <td>
            @Html.ActionLink("Rediger", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detaljer", "Details", new {  id=item.Id }) |
            @Html.ActionLink("Slett", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
